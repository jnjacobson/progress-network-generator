{
  "name": "@jnjacobson/progress-network-generator",
  "version": "3.0.0",
  "description": "Generate progress networks from raw data.",
  "main": "dist/index.js",
  "types:": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist & tsc",
    "build:docs": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "vuetsc": "vue-tsc --noEmit",
    "lint": "eslint {src,docs}/**/*.{ts,vue}",
    "lint:fix": "eslint --fix {src,docs}/**/*.{ts,vue}",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "yarn test && yarn lint && yarn tsc && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnjacobson/progress-network-generator.git"
  },
  "author": "Jan Jacobson",
  "dependencies": {
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0"
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^8.7.1",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "vite": "^2.8.4",
    "vitest": "^0.9.3",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.10"
  }
}
