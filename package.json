{
  "name": "@jnjacobson/progress-network-generator",
  "version": "1.0.0",
  "description": "Generate progress networks from raw data.",
  "main": "dist/index.js",
  "types:": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "build:docs": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "vuetsc": "vue-tsc --noEmit",
    "lint": "eslint {src,docs}/**/*.{ts,vue}",
    "lint:fix": "eslint --fix {src,docs}/**/*.{ts,vue}",
    "test": "vitest run",
    "test:watch": "vitest",

    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnjacobson/progress-network-generator.git"
  },
  "author": "Jan Jacobson",
  "dependencies": {
    "d3": "^7.3.0"
  },
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2",
    "vite": "^2.8.4",
    "vitest": "^0.9.3",
    "vue": "^3.2.31",
    "vue-tsc": "^0.31.4"
  }
}
