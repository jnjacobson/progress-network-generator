{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["vite/client"],
    "skipLibCheck": true,
    "outDir": "./dist"
  },

  "include": ["**/*.ts", "**/*.d.ts", "**/*.vue"],
}
